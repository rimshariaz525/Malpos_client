{
  "logo": {
    "path": "/ecommerce",
    "src": "images/logo-malpos.png",
    "alt": "yello",
    "name": "malpos"
  },
  "pattern": {
    "src": "images/pattern.webp",
    "alt": "line-pattern"
  },
  "title": "Login to MALPOS",
  "input": [
    {
      "icon": "email",
      "type": "email",
      "name": "email",
      "placeholder": "Enter your email",
      "fieldSize": "w-100 h-sm"
    },
    {
      "icon": "lock",
      "type": "password",
      "name": "password",
      "placeholder": "Enter your password",
      "fieldSize": "w-100 h-sm",
      "passwordVisible": true
    }
  ],
  "button": { "type": "submit", "text": "sign in", "fieldSize": "h-sm" }
}
